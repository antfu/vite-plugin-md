{
  "name": "vite-plugin-md",
  "version": "0.12.4",
  "description": "Markdown for Vite",
  "keywords": [
    "vite",
    "vite-plugin",
    "markdown-it"
  ],
  "homepage": "https://github.com/antfu/vite-plugin-md",
  "bugs": "https://github.com/antfu/vite-plugin-md/issues",
  "license": "MIT",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vite-plugin-md"
  },
  "funding": "https://github.com/sponsors/antfu",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "dev": "npm run build -- --watch",
    "example:build": "cp README.md example/README.md && npm -C example run build",
    "example:dev": "cp README.md example/README.md && npm -C example run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:update": "vitest -u"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  },
  "dependencies": {
    "@antfu/utils": "^0.5.1",
    "@rollup/pluginutils": "^4.2.0",
    "@types/markdown-it": "^12.2.3",
    "@types/prismjs": "^1.26.0",
    "@vue/runtime-core": "^3.2.32",
    "gray-matter": "^4.0.3",
    "markdown-it": "^12.3.2"
  },
  "optionalDependencies": {
    "prismjs": "^1.27.0",
    "shiki": "^0.10.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@antfu/ni": "^0.14.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitest/ui": "^0.9.3",
    "@vue/test-utils": "^2.0.0-rc.20",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "fp-ts": "^2.11.9",
    "happy-dom": "^2.55.0",
    "prismjs": "^1.27.0",
    "rollup": "^2.70.1",
    "shiki": "^0.10.1",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "vite": "^2.9.3",
    "vite-plugin-pages": "^0.22.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vitest": "^0.9.3",
    "vue": "^3.2.32",
    "vue-router": "^4.0.14"
  }
}
