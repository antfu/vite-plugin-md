{
  "name": "vite-plugin-md",
  "version": "0.11.4",
  "description": "Markdown for Vite",
  "keywords": [
    "vite",
    "vite-plugin",
    "markdown-it"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vite-plugin-md"
  },
  "funding": "https://github.com/sponsors/antfu",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/antfu/vite-plugin-md",
  "bugs": "https://github.com/antfu/vite-plugin-md/issues",
  "scripts": {
    "dev": "npm run build -- --watch",
    "example:dev": "cp README.md example/README.md && npm -C example run dev",
    "example:build": "cp README.md example/README.md && npm -C example run build",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "test": "jest",
    "test:update": "jest -u",
    "prepublishOnly": "npm run build",
    "release": "git-ensure --no-behind -b main --clean && bumpp --commit --tag --push"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "@types/markdown-it": "^12.2.3",
    "gray-matter": "^4.0.3",
    "markdown-it": "^12.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.10.0",
    "@antfu/ni": "^0.10.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.1.0",
    "eslint-plugin-jest": "^25.2.2",
    "git-ensure": "^0.1.0",
    "jest": "^27.3.1",
    "rollup": "^2.58.3",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.7",
    "tsup": "^5.5.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.13"
  }
}
